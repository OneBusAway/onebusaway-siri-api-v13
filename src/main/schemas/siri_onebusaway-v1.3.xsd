<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns="http://onebusaway.org/siri" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://onebusaway.org/siri"  elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0" id="onebusaway_siri">  <xsd:element name="OneBusAwayConsequence">    <xsd:annotation>      <xsd:documentation>Custom element capturing our extensions to the SIRI SX Consequence element.</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:complexContent>        <xsd:extension base="OneBusAwayConsequenceStructure" />      </xsd:complexContent>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="OneBusAwayConsequenceStructure">    <xsd:annotation>      <xsd:documentation>Custom structure capturing our extensions to the SIRI SX Consequence element.      </xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="DiversionPath" type="xsd:string" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Encoded polyline representation of a diversion/reroute path</xsd:documentation>        </xsd:annotation>      </xsd:element>    </xsd:sequence>  </xsd:complexType>  <xsd:element name="OneBusAwayAffects">    <xsd:annotation>      <xsd:documentation>Custom element capturing our extensions to the SIRI SX Affects element.</xsd:documentation>    </xsd:annotation>    <xsd:complexType>      <xsd:complexContent>        <xsd:extension base="OneBusAwayAffectsStructure" />      </xsd:complexContent>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="OneBusAwayAffectsStructure">    <xsd:annotation>      <xsd:documentation>Type for our custom OneBusAwayAffects structure.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="Applications" minOccurs="0">        <xsd:annotation>          <xsd:documentation>Specific applications affected by situation.</xsd:documentation>        </xsd:annotation>        <xsd:complexType>          <xsd:sequence>            <xsd:element name="AffectedApplication" type="AffectedApplicationStructure" maxOccurs="unbounded" />          </xsd:sequence>        </xsd:complexType>      </xsd:element>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="AffectedApplicationStructure">    <xsd:annotation>      <xsd:documentation>Type for our an affected application.</xsd:documentation>    </xsd:annotation>    <xsd:sequence>      <xsd:element name="ApiKey" type="xsd:string" minOccurs="0">        <xsd:annotation>          <xsd:documentation>The API key for the affected application</xsd:documentation>        </xsd:annotation>      </xsd:element>    </xsd:sequence>  </xsd:complexType></xsd:schema>